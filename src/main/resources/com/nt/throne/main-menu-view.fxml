<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.nt.throne.controller.MainController"
            prefHeight="720.0" prefWidth="1280.0">
    <StackPane prefHeight="720.0" prefWidth="1280.0">
        <MediaView fx:id="videoBackground" fitHeight="720.0" fitWidth="1280.0" />
        <Canvas fx:id="canvas" height="720.0" width="1280.0" />
        <VBox alignment="CENTER">
            <ImageView fitHeight="224.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
                <Image url="@../../../../resources/com/nt/throne/Menu/logo.png"/>
            </ImageView>
            <Text text="Text"/>
        </VBox>
    </StackPane>
</AnchorPane>